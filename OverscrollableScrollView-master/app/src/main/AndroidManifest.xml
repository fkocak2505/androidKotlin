<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.worksplash.akscorp.myapplication">

    <application
            android:allowBackup="true"
            android:label="OverscrollTest"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:fullBackupContent="false">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".examples_activity.WithHeaderScale"/>
        <activity android:name=".examples_activity.WithHeaderNoScale"/>
        <activity android:name=".examples_activity.WithoutHeader"/>
    </application>

</manifest>